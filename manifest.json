{
  "name": "Jade Bot | Cadbury's",
  "version": "1.3",
  "manifest_version": 2,
  "description": "Bypass the 5 per-household limit on Cadbury's free chocolate bars",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "background": {
    "page": "main.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "image.png"
  }
}